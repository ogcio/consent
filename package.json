{
  "name": "@ogcio/consent-monorepo",
  "version": "0.1.0",
  "private": true,
  "description": "Monorepo for OGCIO consent management package and examples",
  "scripts": {
    "build": "pnpm -r build",
    "dev": "pnpm -r --parallel dev",
    "clean": "pnpm -r clean",
    "type-check": "pnpm -r type-check",
    "fix:linting": "pnpm -r fix:linting",
    "check:formatting": "pnpm -r check:formatting",
    "check:linting": "pnpm -r check:linting",
    "fix:formatting": "pnpm -r fix:formatting",
    "test": "pnpm -r test",
    "test:watch": "pnpm -r test:watch",
    "test:coverage": "pnpm -r test:coverage",
    "preinstall": "pnpm --filter @ogcio/consent preinstall",
    "prepare": "pnpm --filter @ogcio/consent prepare",
    "prepublishOnly": "pnpm --filter @ogcio/consent prepublishOnly"
  },
  "keywords": [
    "consent",
    "gdpr",
    "privacy",
    "nextjs",
    "react",
    "typescript",
    "monorepo"
  ],
  "author": "OGCIO",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ogcio/consent.git"
  },
  "bugs": {
    "url": "https://github.com/ogcio/consent/issues"
  },
  "homepage": "https://github.com/ogcio/consent#readme",
  "devDependencies": {
    "@biomejs/biome": "^2.2.4",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@types/node": "^24.5.2",
    "concurrently": "^9.2.1",
    "husky": "^9.1.7",
    "nodemon": "^3.1.10",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.20.5",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
